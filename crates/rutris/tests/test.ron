Rutris {
    units: {
        "witcher3": Unit {
            name: "The Witcher 3",
            prefix: None,
            steamgriddb_id: Some(
                8093,
            ),
            cd: Some(
                "/mnt/files/Games/The Witcher 3 Wild Hunt/bin/x64/",
            ),
            command: [
                "witcher3.exe",
                "--help",
            ],
            winetricks: [
                "vcrun2015",
            ],
            mounts: {
                'd': "/etc",
            },
            before: [],
            generate: Generate {
                sunshine: true,
                desktop: true,
                shell: false,
            },
            env: {
                "DXVK_ASYNC": "1",
                "DXVK_CONFIG_FILE": "/home/user/.config/play.sh/dxvk.conf",
                "DXVK_ENABLE_NVAPI": "1",
                "MANGOHUD_CONFIG": "no_display,vram,gpu_temp,gpu_core_clock,frametime",
                "VKDED_CONFIG": "dxr",
                "WINEESYNC": "0",
                "WINE_HIDE_NVIDIA_GPU": "0",
            },
            wrapper: [
                "gamemoderun",
                "mangohud",
            ],
            runtime: GeProton {
                version: ReleaseVersion(
                    "*",
                ),
            },
            libraries: {
                DxvkGplAsync: ReleaseVersion(
                    "*",
                ),
                DxvkNvapi: ReleaseVersion(
                    "*",
                ),
                Vkd3dProton: ReleaseVersion(
                    "*",
                ),
            },
        },
    },
}
